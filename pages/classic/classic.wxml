<!--pages/classic.wxml-->
<view class="header">
  <v-epsoide index="{{classicData.index}}" class="epsoide" />
  <v-like class="like" bind:like="onLike" like="{{classicData.like_status}}" count="{{classicData.fav_nums}}" />
</view>
<v-movie img="{{classicData.image}}" content="{{classicData.content}}"/>
